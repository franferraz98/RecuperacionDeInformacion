<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF 
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xml:base="http://www.trabajos.fake/trabajos#"
    xmlns:dc="http://purl.org/dc/elements/1.1/">

    <skos:ConceptScheme rdf:about="http://www.trabajos.fake/skos">
        <dc:title>Modelo terminologico</dc:title>
        <dc:description>Modelo terminologico basado en el esquema SKOS</dc:description>
    </skos:ConceptScheme>

    <skos:Concept rdf:about="http://www.trabajos.fake/skos#medicina">
		<skos:definition>Ciencia dedicada al estudio de la vida, la salud y las enfermedades del ser humano</skos:definition>
		<skos:prefLabel xml:lang="es">medicina</skos:prefLabel>
		<skos:altLabel>patologia</skos:altLabel>
		<skos:altLabel>farmaco</skos:altLabel>
        <skos:altLabel>oftalmologia</skos:altLabel>
        <skos:altLabel>cirugia</skos:altLabel>
        <skos:altLabel>medicamento</skos:altLabel>
	</skos:Concept>

    <skos:Concept rdf:about="http://www.trabajos.fake/skos#enfermedad">
		<skos:definition>Alteracion mas o menos grave de la salud</skos:definition>
		<skos:prefLabel xml:lang="es">enfermedad</skos:prefLabel>
		<skos:altLabel>patologia</skos:altLabel>
		<skos:altLabel>afeccion</skos:altLabel>
		<skos:altLabel>trastorno</skos:altLabel>
        <skos:altLabel>dolencia</skos:altLabel>
	</skos:Concept>

    <skos:Concept rdf:about="http://www.trabajos.fake/skos#ocular">
		<skos:definition>De los ojos o relacionado con ellos</skos:definition>
		<skos:prefLabel xml:lang="es">ocular</skos:prefLabel>
		<skos:altLabel>oculares</skos:altLabel>
		<skos:altLabel>oftalmico</skos:altLabel>
        <skos:altLabel>optico</skos:altLabel>
	</skos:Concept>

    <skos:Concept rdf:about="http://www.trabajos.fake/skos#evolucion">
		<skos:definition>Dicho de una teoria o de una idea: Desarrollarse o transformarse</skos:definition>
		<skos:prefLabel xml:lang="es">evolucion</skos:prefLabel>
		<skos:altLabel>transformacion</skos:altLabel>
		<skos:altLabel>desarrollo</skos:altLabel>
		<skos:altLabel>progreso</skos:altLabel>
	</skos:Concept>

    <skos:Concept rdf:about="http://www.trabajos.fake/skos#corriente">
		<skos:definition>Curso, movimiento o tendencia de los sentimientos o de las ideas</skos:definition>
		<skos:prefLabel xml:lang="es">corriente</skos:prefLabel>
		<skos:altLabel>flujo</skos:altLabel>
	</skos:Concept>

    <skos:Concept rdf:about="http://www.trabajos.fake/skos#ideologica">
		<skos:definition>Perteneciente o relativo a la ideologia</skos:definition>
		<skos:prefLabel xml:lang="es">ideologica</skos:prefLabel>
		<skos:altLabel>ideologia</skos:altLabel>
		<skos:altLabel>doctrina</skos:altLabel>
	</skos:Concept>

    <skos:Concept rdf:about="http://www.trabajos.fake/skos#siglo">
		<skos:definition>Periodo de cien años</skos:definition>
		<skos:prefLabel xml:lang="es">siglo</skos:prefLabel>
	</skos:Concept>

    <skos:Concept rdf:about="http://www.trabajos.fake/skos#cine">
		<skos:definition>Tecnica, arte e industria de la cinematografia</skos:definition>
		<skos:prefLabel xml:lang="es">cine</skos:prefLabel>
		<skos:altLabel>pelicula</skos:altLabel>
        <skos:altLabel>cinematografia</skos:altLabel>
	</skos:Concept>

    <skos:Concept rdf:about="http://www.trabajos.fake/skos#desarrollo">
		<skos:definition>Accion y efecto de desarrollar o desarrollarse</skos:definition>
		<skos:prefLabel xml:lang="es">desarrollo</skos:prefLabel>
		<skos:altLabel>progreso</skos:altLabel>
        <skos:altLabel>avance</skos:altLabel>
        <skos:altLabel>mejora</skos:altLabel>
	</skos:Concept>

    <skos:Concept rdf:about="http://www.trabajos.fake/skos#videojuego">
		<skos:definition>Juego electronico que se visualiza en una pantalla.</skos:definition>
		<skos:prefLabel xml:lang="es">videojuego</skos:prefLabel>
		<skos:altLabel>juego</skos:altLabel>
	</skos:Concept>

    <skos:Concept rdf:about="http://www.trabajos.fake/skos#diseño">
		<skos:definition>Concepcion original de un objeto u obra destinados a la produccion en serie</skos:definition>
		<skos:prefLabel xml:lang="es">diseño</skos:prefLabel>
	</skos:Concept>

    <skos:Concept rdf:about="http://www.trabajos.fake/skos#personaje">
		<skos:definition>Seres reales o imaginarios que figuran en una obra</skos:definition>
		<skos:prefLabel xml:lang="es">personaje</skos:prefLabel>
	</skos:Concept>

	<skos:Concept rdf:about="http://www.trabajos.fake/skos#inteligencia">
		<skos:definition>Disciplina cientifica que se ocupa de crear programas 
			informaticos que ejecutan operaciones comparables a las que realiza
			la mente humana, como el aprendizaje o el razonamiento logico.</skos:definition>
		<skos:prefLabel xml:lang="es">inteligencia</skos:prefLabel>
	</skos:Concept>

    <skos:Concept rdf:about="http://www.trabajos.fake/skos#artificial">
		<skos:definition>Creado por el ser humano</skos:definition>
		<skos:prefLabel xml:lang="es">artifcial</skos:prefLabel>
		<skos:altLabel>simulado</skos:altLabel>
        <skos:altLabel>sintetico</skos:altLabel>
	</skos:Concept>

    <skos:Concept rdf:about="http://www.trabajos.fake/skos#contaminacion">
		<skos:definition>Accion y efecto de contaminar</skos:definition>
		<skos:prefLabel xml:lang="es">contaminacion</skos:prefLabel>
		<skos:altLabel>polucion</skos:altLabel>
	</skos:Concept>

    <skos:Concept rdf:about="http://www.trabajos.fake/skos#informatica">
		<skos:definition>Tratamiento automatico de la informacion por medio de computadoras</skos:definition>
		<skos:prefLabel xml:lang="es">informatica</skos:prefLabel>
		<skos:altLabel>computacion</skos:altLabel>
        <skos:altLabel>ordenador</skos:altLabel>
	</skos:Concept>

</rdf:RDF>